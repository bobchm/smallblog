<%- include("partials/header") -%>

<h1>Home</h1>

<p><%= homeContent %></p>

<% posts.forEach(function(post) { %>
    <h1><%= post.title %></h1>
    <p class="readmore"><%= post.content.slice(0, 100) + "..." %></p>
    <a href=<%= "/posts/" + post.title.replaceAll(" ", "-") %>>Read More</a>
<% }) %>

<%- include("partials/footer") -%>

